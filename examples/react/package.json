    "react-scripts-ts": "2.16.0",
    "styled-components": "^3.2.5"
  },
  "devDependencies": {
    "@storybook/react": "^3.4.0",
    "@types/chalk": "^2.2.0",
    "@types/jest": "^23.3.0",
    "@types/node": "^10.5.3",
    "@types/react": "^16.4.7",
    "@types/react-dom": "^16.0.6",
    "@types/storybook__addon-backgrounds": "^3.2.0",
    "@types/storybook__react": "^3.0.7"
  }
}
