{
  "name": "storybook-chrome-screenshot",
  "version": "0.9.1",
  "description": "A Storybook addon, Save the screenshot image of your stories! via puppeteer.",
  "main": "lib/index.js",
  "bin": {
    "storybook-chrome-screenshot": "lib/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "npm run test:lint && npm run test:unit && npm run test:e2e",
    "test:lint": "tslint \"src/**/*.ts\" \"src/**/*.tsx\"",
    "test:unit": "jest",
    "test:e2e": "bash scripts/e2e.sh",
    "test:watch": "npm run test:unit -- --watch",
    "clean": "rm -rf lib",
    "prebuild": "npm run clean",
    "prescreenshot": "npm run build",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsuyoshiwada/storybook-chrome-screenshot.git"
  },
  "author": "tsuyoshiwada",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tsuyoshiwada/storybook-chrome-screenshot/issues"
  },
  "homepage": "https://github.com/tsuyoshiwada/storybook-chrome-screenshot#readme",
  "files": [
    "lib",
    "CHANGELOG.md",
    "README.md",
    "LICENSE",
    "package.json",
    "register.js"
  ],
  "keywords": [
    "storybook",
    "addon",
    "puppeteer",
    "chrome",
    "screenshot",
    "capture",
    "visual-testing"
  ],
  "devDependencies": {
    "@storybook/addon-backgrounds": "^3.3.3",
    "@storybook/react": "^3.3.3",
    "@types/enzyme": "^3.1.6",
    "@types/enzyme-adapter-react-16": "^1.0.1",
    "@types/jest": "^22.0.1",
    "@types/node": "^8.5.2",
    "@types/react": "^16.0.31",
    "@types/react-dom": "^16.0.3",
    "babel-core": "^6.26.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^22.0.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "tslint": "^5.8.0",
    "tslint-react": "^3.3.3",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "@types/commander": "^2.12.2",
    "@types/imagesloaded": "^4.1.1",
    "@types/lodash": "^4.14.91",
    "@types/log-symbols": "^2.0.0",
    "@types/mkdirp": "^0.5.2",
    "@types/node-emoji": "^1.8.0",
    "@types/progress": "^2.0.1",
    "@types/puppeteer": "^0.13.9",
    "@types/query-string": "^5.0.1",
    "@types/sanitize-filename": "^1.1.28",
    "@types/storybook__react": "^3.0.5",
    "babel-polyfill": "^6.26.0",
    "chalk": "^2.3.0",
    "commander": "^2.12.2",
    "imagesloaded": "^4.1.3",
    "lodash": "^4.17.4",
    "log-symbols": "^2.1.0",
    "mkdirp": "^0.5.1",
    "node-emoji": "^1.8.1",
    "progress": "^2.0.0",
    "puppeteer": "^0.13.0",
    "query-string": "^5.0.1",
    "sanitize-filename": "^1.6.1",
    "util-inspect": "^0.1.8"
  },
  "peerDependencies": {
    "@storybook/addons": ">=3.1.0",
    "react": ">=15.0.0 || ^16.0.0-alpha",
    "react-dom": ">=15.0.0 || ^16.0.0-alpha"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jest/setupTests.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/jest/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.test.(ts|tsx)"
    ]
  }
}
